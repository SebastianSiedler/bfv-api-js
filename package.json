{
  "name": "bfv-api",
  "version": "1.3.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "pnpm openapi-zod-client \"./bfv_schema.yaml\" -o \"./src/client.ts\" --base-url \"https://widget-prod.bfv.de/api/service/widget/v1\"",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "test": "pnpm generate && vitest run"
  },
  "keywords": [
    "bfv",
    "api",
    "typescript",
    "zod"
  ],
  "author": "",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianSiedler/bfv-api-js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.8.2",
    "openapi-zod-client": "^1.13.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@zodios/core": "^10.9.6",
    "zod": "^3.22.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
